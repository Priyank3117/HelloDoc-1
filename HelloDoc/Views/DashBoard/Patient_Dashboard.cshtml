@model List<DAL.ViewModel.Patient_Dash>
<div class="tab-pane fade show active " id="pills-home" role="tabpanel" aria-labelledby="pills-home-tab" tabindex="0" style="background-color:#E9ECEF">

    <div class="mx-auto pt-5" style="width:95%;">
       <h3>Medical History</h3>
       <div class="d-flex justify-content-end bg-white">
            <button type="button" class="btn btn-outline-info mx-3 my-2">Create New Request</button>
       </div>
        <table class="table">
            <thead>
                <tr style="background-color:#E9ECEF">
                    <th scope="col">Created Date</th>
                    <th scope="col">Current Status</th>
                    <th scope="col">Documents</th>
                </tr>
            </thead>
            <tbody>
               @foreach (var item in Model)
                {
                    <tr>
                    <td>@Html.DisplayFor(Modelitem => item.CreatedDate)</td>
                    </tr>
                }
            </tbody>
        </table>

   </div>
    
 

</div>
